<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:cool="http://AFECSHOMEDB/schema/cool#" >

    <!--DO NOT MODIFY... CRITICAL STATE MACHINE CONFIGURATION!-->
    <!---->
  <rdf:Description rdf:about="http://AFECSHOMEDB/Service/CodaRC/CodaRcGo#CodaRcGo">
      <cool:hasName>CodaRcGo</cool:hasName>
      <cool:hasDescription>Coda go state transition</cool:hasDescription>
      <cool:startsStatemachine rdf:resource="#RC_Go"/>
  </rdf:Description>

    <cool:Rule rdf:ID="RC_Go">
        <!--used for supervisor final state definition '+ed'-->
       <cool:hasName>active</cool:hasName>
       <cool:hasDescription>Description of the Coda go transition</cool:hasDescription>
       <cool:hasCode>

           if ( priority++ in_state paused ) {
           priority move_to active;
           }


       </cool:hasCode>
    </cool:Rule>

</rdf:RDF>
