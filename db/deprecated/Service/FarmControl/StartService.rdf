<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:cool="http://AFECSHOMEDB/schema/cool#" >

    <!--DO NOT MODIFY... CRITICAL STATE MACHINE CONFIGURATION!-->
    <!---->
  <rdf:Description rdf:about="http://AFECSHOMEDB/Service/FarmControl/StartService#StartService">
      <cool:hasName>StartService</cool:hasName>
      <cool:hasDescription>Farm node Start service</cool:hasDescription>
      <cool:startsStatemachine rdf:resource="#RC_Start"/>
  </rdf:Description>

    <cool:Rule rdf:ID="RC_Start">
        <!--used for supervisor final state definition '+ed'-->
       <cool:hasName>start</cool:hasName>
       <cool:hasDescription>Description farm control start service</cool:hasDescription>
       <cool:hasCode>

           if ( (type_USER in_state configured) || (type_USER in_state stoped))  {
           type_USER move_to started;
           } 


       </cool:hasCode>
    </cool:Rule>

</rdf:RDF>
